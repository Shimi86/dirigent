    <Plan Name="plan1">
    	<App
    	    AppIdTuple = "m1.a"
    		Template = "apps.notepad"
    		Disabled = "1"
    		StartupDir = "c:\"
    		CmdLineArgs = "m1.a.txt"
    		WindowStyle = "normal"
    		AdoptIfAlreadyRunning = "0"
    		RestartOnCrash = "0"
    		PriorityClass = "belownormal"
    	    >
    	    <InitDetectors>
    	      <WindowPoppedUp TitleRegExp="\s-\sNotepad"/>
    	      <TimeOut>5.0</TimeOut>
    	    </InitDetectors>
    		<!--WindowPos TitleRegExp="\s-\sNotepad" BringToFront="1" keep="1"/-->
    		<!--WindowPos TitleRegExp="\s-\sNotepad" SendToBack="1"/-->
    		<!--WindowPos TitleRegExp="\s-\sNotepad" TopMost="1" /-->
    		<!--WindowPos TitleRegExp="\s-\sNotepad" WindowStyle="hidden" /-->
    		<!--WindowPos TitleRegExp="\s-\sNotepad" Rect="10,50,300,200"/-->
    		<!--WindowPos TitleRegExp="\s-\sNotepad" Rect="10,50,300,200" Screen="1" Keep="0" WindowStyle="maximized" BringToFront="1" /-->
    		<!--WindowPos TitleRegExp="^Notepad$" Rect="10,50,300,200" Screen="0" Keep="0" /-->
    	</App>

    	<App
    	    AppIdTuple = "m1.b"
    		Template = "apps.notepad"
    		StartupDir = "c:\"
    		CmdLineArgs = "m1.b.txt"
    		RestartOnCrash="1"
    	    >
    		<WindowPos titleregexp="\s-\sNotepad" rect="10,450,300,200" screen="1" keep="0" />
    		<Restarter maxTries="2" delay="5"/>
    		<SoftKill>
    		   <Keys Timeout="1.5" Keys="closing{ESC}"/>
    		   <Close Timeout="0.7"/>
    		</SoftKill>
    	</App>

    	<App
    	    AppIdTuple = "m2.a"
    		Template = "apps.notepad"
    		StartupDir = "c:\"
    		CmdLineArgs = "m2.a.txt"
    	    >
    		<WindowPos titleregexp="\s-\sNotepad" rect="410,50,300,200" screen="1" keep="0" />
    	</App>

    	<App
    	    AppIdTuple = "m2.b"
    		Template = "apps.notepad"
    		StartupDir = "c:\"
    		CmdLineArgs = "m2.b.txt"
    	    >
    		<WindowPos titleregexp="\s-\sNotepad" rect="410,450,300,200" screen="1" keep="0" />
    	</App>
    
    	<App
    	    AppIdTuple = "m1.ps1"
    		ExeFullPath = "[powershell.command]"
    		StartupDir = "c:\"
    		CmdLineArgs = "ls"
    		Volatile = "1"
    	    >
    	</App>

    	<App
    	    AppIdTuple = "m1.ps2"
    		ExeFullPath = "[powershell.file]"
    		StartupDir = ""
    		CmdLineArgs = "%DIRIGENT_SHAREDCONFDIR%\test1.ps1"
    		Volatile = "1"
    	    >
    	</App>

    	<App
    	    AppIdTuple = "m1.batch1"
    		ExeFullPath = "[cmd.file]"
    		StartupDir = ""
    		CmdLineArgs = "%DIRIGENT_SHAREDCONFDIR%\test1.bat"
    		Volatile = "1"
    	    >
    		<SoftKill>
    		   <Keys Timeout="3.5" Keys="^(c)"/>
    		   <Close Timeout="0.5"/>
    		</SoftKill>
    	</App>

    	<App
    	    AppIdTuple = "m1.batch-cmd"
    		ExeFullPath = "[cmd.command]"
    		StartupDir = ""
    		CmdLineArgs = "dir"
    		Volatile = "1"
    	    >
    	</App>

    	<App
    	    AppIdTuple = "m1.dirig-cmd"
    		ExeFullPath = "[dirigent.command]"
    		StartupDir = ""
    		CmdLineArgs = "LaunchApp m1.a"
    		Volatile = "1"
    	    >
    	</App>

    </Plan>

    <Plan Name="plan2">
    	<App
    	    AppIdTuple = "m1.c"
    		Template = "apps.notepad"
    		StartupDir = "c:\"
    		CmdLineArgs = "ccc.txt"
    	/>

    	<App
    	    AppIdTuple = "m1.d"
    		Template = "apps.notepad"
    		StartupDir = "c:\"
    		CmdLineArgs = "ddd.txt"
    	/>
    </Plan>

    <Plan Name="plan3">
    	<App
    	    AppIdTuple = "m1.dc1"
    		ExeFullPath = "[dirigent.command]"
    		CmdLineArgs = "SetVars X=1; StartPlan plan2; RestartApp m1.batch1"
    	/>
    </Plan>

    <AppTemplate Name="powershell"
    		Template = ""
    		ExeFullPath = "%windir%\system32\WindowsPowerShell\v1.0\powershell.exe"
    		StartupDir = "c:\"
    		CmdLineArgs = ""
    		StartupOrder = "0"
    		RestartOnCrash = "0"
    		SeparationInterval = "0.5"
    		KillTree = "1"
    />

    <AppTemplate Name="apps.notepad"
    		Template = ""
    		ExeFullPath = "c:\windows\notepad.exe"
    		StartupDir = "c:\"
    		CmdLineArgs = ""
    		StartupOrder = "0"
    		RestartOnCrash = "1"
    		SeparationInterval = "0.5"
    		KillTree = "0"
			KillSoftly = "0"
    />

</Shared>
